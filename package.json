    "cookie-parser": "~1.4.3",
    "debug": "~2.6.9",
    "express": "^4.16.4",
    "http-errors": "~1.6.2",
    "morgan": "^1.9.1",
    "nano": "^6.4.4",
    "nodemon": "^1.18.9",
    "object-hash": "^1.3.1",
    "pug": "^2.0.3",
    "sass": "^1.17.0",
    "spectre.css": "^0.5.8",
    "time-stamp": "^2.2.0",
    "validator": "^10.11.0"
